<script type="text/template" id="user">
  <div class="panel panel-default">
    <div class="panel-body">
      <form class='update-user' >
        <div class="form-group">
          <label for="first_name">First Name</label>
          <input type="text" class="form-control" name="first_name" value="{{ getFirst_name this}}" /required>
        </div>
        <div class="form-group">
          <label for="last_name">Last Name</label>
          <input type="text" class="form-control" name="last_name" value="{{ getLast_name this}}" /required>
        </div>
        <div class="form-group">
          <label for="note">Note</label>
          <input type="text" class="form-control" name="note" value="{{ getNote this}}" >
        </div>
        <button type="submit" class="btn btn-primary">Save</button>
        <a class='cancel btn btn-danger'>Cancel</a>
      </form>
    </div>
  </div>
</script>
